<tlc:customtreeconfig version="VERSION_2_2" xmlns:tlc="http://sblim.org/wbemsmt/tasklauncher/customtreeconfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://sblim.org/wbemsmt/tasklauncher/customtreeconfig ../../wbemsmt-commons/commons-bl/conf/customtreeconfig.xsd">
	<tlc:contextmenu>
		<tlc:menuitem>
			<tlc:description>tree.menu.createShare</tlc:description>
			<tlc:eventListener>
				<tlc:classname>org.sblim.wbemsmt.samba.listener.CreateShareListener</tlc:classname>
			</tlc:eventListener>
		</tlc:menuitem>
		<tlc:menuitem>
			<tlc:description>tree.menu.createPrinter</tlc:description>
			<tlc:eventListener>
				<tlc:classname>org.sblim.wbemsmt.samba.listener.CreatePrinterListener</tlc:classname>
			</tlc:eventListener>
		</tlc:menuitem>	
		<tlc:menuitem>
			<tlc:description>tree.menu.createUser</tlc:description>
			<tlc:eventListener>
				<tlc:classname>org.sblim.wbemsmt.samba.listener.CreateUserListener</tlc:classname>
			</tlc:eventListener>
		</tlc:menuitem>
	</tlc:contextmenu>
	<tlc:treenode>
		<tlc:description>root</tlc:description>
		<tlc:treenode>
			<tlc:description>tree.samba</tlc:description>
			<tlc:eventListener isSystemListener="true">
				<tlc:classname>org.sblim.wbemsmt.tasklauncher.InitAdapterListener</tlc:classname>
				<tlc:param>
					<tlc:param-name>adapterClassName</tlc:param-name>
					<tlc:param-value>org.sblim.wbemsmt.samba.bl.adapter.SambaCimAdapter</tlc:param-value>
				</tlc:param>
				<tlc:param>
					<tlc:param-name>taskName</tlc:param-name>
					<tlc:param-value>SAMBA</tlc:param-value>
				</tlc:param>
			</tlc:eventListener>
			<tlc:treenode>
				<tlc:description>tree.sambaservice</tlc:description>
				<tlc:cimclass>
					<tlc:classname>Linux_SambaService</tlc:classname>
					<tlc:instanceSubnodes>
						<tlc:treenode>
							<tlc:description>tree.shares</tlc:description>
							<tlc:eventListener>
								<tlc:classname>org.sblim.wbemsmt.samba.listener.EditShareGlobalsListener</tlc:classname>
							</tlc:eventListener>
							<tlc:cimclass>
								<tlc:classname>Linux_SambaShareForService</tlc:classname>
								<tlc:association sourceReferenceName="GroupComponent" targetReferenceName="PartComponent" targetClassName="Linux_SambaShareOptions"/>
								<tlc:instanceSubnodes>
									<tlc:eventListener>
										<tlc:classname>org.sblim.wbemsmt.samba.listener.EditShareListener</tlc:classname>
									</tlc:eventListener>
									<tlc:contextmenu>
										<tlc:menuitem>
											<tlc:description>tree.menu.deleteShare</tlc:description>
											<tlc:eventListener>
												<tlc:classname>org.sblim.wbemsmt.samba.listener.DeleteShareListener</tlc:classname>
											</tlc:eventListener>
										</tlc:menuitem>
									</tlc:contextmenu>
								</tlc:instanceSubnodes>
								<tlc:instanceNamingKey>Name</tlc:instanceNamingKey>
							</tlc:cimclass>
						</tlc:treenode>
						<tlc:treenode>
							<tlc:description>tree.printers</tlc:description>
							<tlc:eventListener>
								<tlc:classname>org.sblim.wbemsmt.samba.listener.EditPrinterGlobalsListener</tlc:classname>
							</tlc:eventListener>
							<tlc:cimclass>
								<tlc:classname>Linux_SambaPrinterForService</tlc:classname>
								<tlc:association sourceReferenceName="GroupComponent" targetReferenceName="PartComponent" targetClassName="Linux_SambaPrinterOptions"/>
								<tlc:instanceSubnodes>
									<tlc:eventListener>
										<tlc:classname>org.sblim.wbemsmt.samba.listener.EditPrinterListener</tlc:classname>
									</tlc:eventListener>
									<tlc:contextmenu>
										<tlc:menuitem>
											<tlc:description>tree.menu.deletePrinter</tlc:description>
											<tlc:eventListener>
												<tlc:classname>org.sblim.wbemsmt.samba.listener.DeletePrinterListener</tlc:classname>
											</tlc:eventListener>
										</tlc:menuitem>
									</tlc:contextmenu>
								</tlc:instanceSubnodes>
								<tlc:instanceNamingKey>Name</tlc:instanceNamingKey>
							</tlc:cimclass>
						</tlc:treenode>
						<tlc:treenode>
							<tlc:description>tree.users</tlc:description>
							<tlc:cimclass>
								<tlc:classname>Linux_SambaUsersForService</tlc:classname>
								<tlc:association sourceReferenceName="GroupComponent" targetReferenceName="PartComponent" targetClassName="Linux_SambaUser"/>
								<tlc:instanceSubnodes>
									<tlc:eventListener>
										<tlc:classname>org.sblim.wbemsmt.samba.listener.EditUserListener</tlc:classname>
									</tlc:eventListener>
									<tlc:contextmenu>
										<tlc:menuitem>
											<tlc:description>tree.menu.deleteUser</tlc:description>
											<tlc:eventListener>
												<tlc:classname>org.sblim.wbemsmt.samba.listener.DeleteUserListener</tlc:classname>
											</tlc:eventListener>
										</tlc:menuitem>
									</tlc:contextmenu>
								</tlc:instanceSubnodes>
								<tlc:instanceNamingKey>SambaUserName</tlc:instanceNamingKey>
							</tlc:cimclass>
						</tlc:treenode>
						<tlc:eventListener>
							<tlc:classname>org.sblim.wbemsmt.samba.listener.EditServiceListener</tlc:classname>
						</tlc:eventListener>
					</tlc:instanceSubnodes>
					<tlc:instanceNamingKey>Name</tlc:instanceNamingKey>
				</tlc:cimclass>
			</tlc:treenode>
		</tlc:treenode>
	</tlc:treenode>
</tlc:customtreeconfig>
